{% extends '@HWIOAuth/layout.html.twig' %}

{% block fos_oauth_social_connect_content %}
    {% if error is defined and error %}
        <span>{{ error }}</span>
    {% endif %}
    {% for owner in fos_oauth_social_connect_resource_owners() %}
    <a href="{{ fos_oauth_social_connect_login_url(owner) }}">{{ owner | trans({}, 'HWIOAuthBundle') }}</a> <br />
    {% endfor %}
{% endblock fos_oauth_social_connect_content %}
